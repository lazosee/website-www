{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/content/*": [
        "src/content/*"
      ],
      "@/db/*": [
        "src/db/*"
      ],
      "@/actions/*": [
        "src/actions/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}